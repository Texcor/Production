    <record model="ir.ui.view" id="texcor_network_mgmt_circuit_form">
      <field name="name">texcor.network.mgmt.circuit.form</field>
      <field name="model">texcor.circuit</field>
      <field name='inherit_id' ref='texcor_network_mgmt_circuit_form_parent'/>
      <field name="arch" type="xml">
        <xpath expr="//group[@name='studio_group_ba0aab']" position="replace">
          <group name="studio_group_33Sa5">
            <group name="studio_group_33Sa5_left" string="Circuit Information">
              <field name="circuit_type"/>
              <field name="pop_id" required="1"/>
              <field name="customer_id" string="Customer" required="1"/>
              <field name="location_id" domain="[[&quot;type&quot;,&quot;=&quot;,&quot;other&quot;]]"/>
              <field name="customer_id" readonly="1"/>
              <field name="service_order_id"/>
              <field name="status" default_value="PreTurnup"/>
              <field name="qos_id"/>
              <field name="vlanid" help="Enter the VLAN ID i.e. 500"/>
              <field name="pe_switch_id" attrs="{&quot;invisible&quot;: [[&quot;circuit_type&quot;,&quot;=&quot;,&quot;Wireless&quot;]]}"/>
              <field name="pe_switch_port" help="Enter the Port Number i.e. 1/0/0" widget="integer" attrs="{&quot;invisible&quot;: [[&quot;circuit_type&quot;,&quot;=&quot;,&quot;Wireless&quot;]]}"/>
              <field name="ap_name_id" string="AP Name" attrs="{&quot;invisible&quot;: [[&quot;circuit_type&quot;,&quot;!=&quot;,&quot;Wireless&quot;]]}" domain="[&quot;|&quot;,[&quot;device_type&quot;,&quot;=&quot;,&quot;Wireless PTP&quot;],[&quot;device_type&quot;,&quot;=&quot;,&quot;Wireless PTMP&quot;]]"/>
              <field name="tower_id" string="Tower" attrs="{&quot;invisible&quot;: [[&quot;circuit_type&quot;,&quot;!=&quot;,&quot;Wireless&quot;]]}" readonly="1"/>
            </group>
            <group name="studio_group_33Sa5_right" string="Customer Site Information">
              <field name="tech_name_id"/>
              <field name="tech_phone"/>
              <field name="tech_email"/>
              <field name="address"/>
              <field name="city"/>
              <field name="state"/>
              <field name="dmarc_location"/>
            </group>
          </group>
          <group name="studio_group_MNgVs">
            <group name="studio_group_MNgVs_left">
              <field name="order_line_ids" string="Order Lines">
                <tree string="Sales Order Lines" create="false">
                  <field name="name" modifiers="{&quot;required&quot;: true}"/>
                  <field name="product_uom_qty" string="Qty" on_change="1" modifiers="{&quot;required&quot;: true}"/>
                  <field name="product_uom" string="Unit of Measure" on_change="1" can_create="true" can_write="true" studio_groups="[{&quot;id&quot;: 15, &quot;name&quot;: &quot;Manage Multiple Units of Measure&quot;, &quot;display_name&quot;: &quot;Technical Settings / Manage Multiple Units of Measure&quot;}]" invisible="1" modifiers="{&quot;invisible&quot;: true, &quot;column_invisible&quot;: true}"/>
                  <field name="route_id" options="{'no_create': True}" on_change="1" can_create="true" can_write="true" studio_groups="[{&quot;id&quot;: 60, &quot;name&quot;: &quot;Enable Route on Sales Order Line&quot;, &quot;display_name&quot;: &quot;Technical Settings / Enable Route on Sales Order Line&quot;}]" invisible="1" modifiers="{&quot;invisible&quot;: true, &quot;column_invisible&quot;: true}"/>
                  <field name="price_subtotal" sum="Total" widget="monetary" modifiers="{&quot;readonly&quot;: true}"/>
                </tree>
              </field>
            </group>
          </group>
          <group name="studio_group_uVYWX" style="width:50%%">
            <group name="studio_group_uVYWX_left" style="width:50%%">
              <field name="ip_management_ids" string="IP Addressing" attrs="{}" readonly="1">
                <tree string="IP Management">
                  <field name="network_cidr" style="width:50%%"/>
                  <field name="net_mask" style="width:50%%"/>
                  <field name="provider_ip" style="width:50%%"/>
                </tree>
              </field>
            </group>
          </group>
          <group name="studio_group_L0oW4">
            <group name="studio_group_L0oW4_left" string="Additional Info">
              <field filename="documents_filename" name="documents"/>
              <field name="notes" widget="text" placeholder="Enter Notes Here"/>
              <field name="sales_person" string="Sales Person"/>
            </group>
            <group name="studio_group_L0oW4_right">
              <field name="create_uid"/>
              <field name="create_date"/>
              <field name="write_uid"/>
              <field name="__last_update"/>
            </group>
          </group>
          <group name="studio_group_maF1f"></group>
        </xpath>
        <xpath expr="//form[1]" position="attributes">
          <attribute name="create">true</attribute>
          <attribute name="delete">true</attribute>
          <attribute name="edit">true</attribute>
        </xpath>
        <xpath expr="//form[1]/sheet[1]" position="before">
          <header>
            <field name="status" string="Circuit Status" widget="statusbar"/>
          </header>
        </xpath>
        <xpath expr="//field[@name='name']" position="attributes">
          <attribute name="attrs">{}</attribute>
          <attribute name="placeholder">Circuit ID</attribute>
          <attribute name="required"/>
          <attribute name="string">CircuitID</attribute>
        </xpath>
      </field>
    </record>

